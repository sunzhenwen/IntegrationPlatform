	<package name="fileUpload" namespace="/" extends="struts-default">
        <action name="fileUpload_*" class="com.tj.jst.outside.business.upload.action.UploadAction" method="{1}">
            <!-- 项目报建 -->
            <result name="constructionSuccess" type="redirect">constructionAction_modifyPage.action?prjId=${id}</result>
            <!-- 勘察申请 -->
            <result name="surveySuccess" type="redirect">surveyAction_modifyPage.action?surveyId=${id}</result>
        	<!-- 设计申请 -->
        	<result name="designSuccess" type="redirect">designAction_modifyPage.action?designId=${id}</result>
        	<!-- 勘察合同备案 -->
        	<result name="surveyContractFiling" type="redirect">surveyContractFilingAction_modifyPage.action?recordId=${id}</result>
        	<!-- 设计合同备案 -->
        	<result name="designContractFiling" type="redirect">designContractFilingAction_modifyPage.action?recordId=${id}</result>
        	<!-- 监理合同备案 -->
        	<result name="supervisContractFiling" type="redirect">supervisorContractFilingAction_modifyPage.action?recordId=${id}</result>
        	<!-- 市建合同备案 -->
        	<result name="theContractorSuccess" type="redirect">theContractorAction_modifyPage.action?recordId=${id}</result>
        	<!-- 发改合同备案 -->
        	<result name="reformContractFiling" type="redirect">reformAction_modifyPage.action?recordId=${id}</result>
        	<!-- 施工分包合同备案 -->
        	<result name="subPackageContrFiling" type="redirect">subPackageAction_modifyPage.action?recordId=${id}</result>
        	<!-- 施工劳务合同备案 -->
        	<result name="labourContractFiling" type="redirect">labourServicesAction_modifyPage.action?recordId=${id}</result>
        	<!-- 施工图审查 -->
        	<result name="constructionPlansSuccess" type="redirect">constructionPlansAction_modifyPage.action?censorId=${id}</result>
        	<!-- 现场安全审查 -->                       
        	<result name="scenesafe" type="redirect">scenesafeAction_modifyPagee.action?permitid=${id}</result>
        	<!-- 质量监督 -->
        	<result name="qualitySuccess" type="redirect">qualityAction_modifyPage.action?qualityId=${id}</result>
        	<!-- 安全监督 -->
        	<result name="safetySuccess" type="redirect">safetyAction_modifyPage.action?safetyId=${id}</result>
        	<!-- 施工许可 -->
        	<result name="permitSuccess" type="redirect">permitAction_modifyPage.action?permitId=${id}</result>
        	<!-- 竣工验收 -->
        	<result name="completeSuccess" type="redirect">completeAction_modifyPage.action?completeId=${id}</result>
        	<!-- 外省企业项目 -->
        	<result name="OtherProjectSuccess" type="redirect">otherProjectAction_modifyPage.action?otherPrjId=${id}</result>
       		<!-- 外省企业项目人员 -->
        	<result name="prjPersonSuccess" type="redirect">projectStaffAction_personDetailed.action?personId=${id}</result>
       		<!-- 项目报建(主管部门修改时使用) -->
            <result name="constrSuccess" type="redirect">constrModifyAction_queryById.action?prjId=${id}</result>
        </action>
    </package>

